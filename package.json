{
  "name": "jcpst",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "marked -o public/about/index.html -i prose/about.md",
    "deploy": "npm run deploy:build -s && npm run deploy:run -s",
    "deploy:build": "docker build --tag jcpst .",
    "deploy:run": "docker run -p 3000:3000 -d jcpst",
    "prod": "pm2 start ./bin/www",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "jade": "~1.9.0",
    "marked": "^0.3.3",
    "serve-favicon": "~2.1.3"
  }
}
