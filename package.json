{
  "name": "jcpst.com",
  "version": "3.0.2",
  "description": "Homepage for Joseph Post",
  "scripts": {
    "build": "harp compile src build",
    "predeploy": "mkdir src/_generated && npm run bash:gen-html && npm run bash:sh-to-md",
    "deploy": "harp compile src /tmp/__temp_static_content",
    "bash:gen-html": "bash ./scripts/parse_fanpage > ./src/_generated/bandcamp-results.html",
    "bash:sh-to-md": "echo '```bash' | cat - scripts/parse_fanpage | sed '$a```' > src/_generated/parse_fanpage.md",
    "start": "harp server src"
  },
  "keywords": [],
  "author": "Joseph Post <joe@jcpst.com> (http://jcpst.com)",
  "license": "ISC",
  "repository": "jcpst/jcpst",
  "dependencies": {
    "harp": "^0.19.0"
  }
}
